<p id="notice"><%= notice %></p>

<p>
  <b>Title:</b>
  <%= @checkin.title %>
</p>

<p>
  <b>Location:</b>
  <%= @checkin.location %>
</p>


<%= link_to 'Edit', edit_checkin_path(@checkin) %> |
<%= link_to 'Back', checkins_path %>

<h2>Nearby check-ins</h2>
<ul>
  <% @nearby_checkins.each do |checkin| %>
    <li><%= link_to checkin.title, checkin %></li>
  <% end %>
</ul>